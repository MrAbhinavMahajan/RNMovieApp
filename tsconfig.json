    "baseUrl": ".",
    "paths": {
      "~/*": ["*"],
      "@apis/*": ["./src/apis/*"],
      "@store/*": ["./src/store/*"],
      "@service/*": ["./src/service/*"],
      "@assets/*": ["./src/assets/*"],
      "@constants/*": ["./src/constants/*"],
